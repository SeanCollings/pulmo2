{
  "expo": {
    "name": "PulmO2",
    "slug": "pulmo2",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lungs_splash.png",
    "splash": {
      "image": "./assets/lungs_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#002f56"
    },
    "packagerOpts": {
      "sourceExts": ["js", "json", "ts", "tsx", "jsx"]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.luft.pulmo-2",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "icon": "./assets/lungs_splash.png"
    },
    "android": {
      "package": "com.luft.pulmo_2",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/lungs_splash.png",
        "backgroundColor": "#002f56"
      },
      "permissions": []
    }
  }
}
