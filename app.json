{
  "expo": {
    "name": "PulmO2",
    "slug": "pulmo2",
    "description": "An app to help you with your Luft breathing excercises.",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pulmo2_icon.png",
    "packagerOpts": {
      "sourceExts": ["js", "json", "ts", "tsx", "jsx"]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.luft.pulmo-2",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "icon": "./assets/pulmo2_icon.png"
    },
    "android": {
      "package": "com.luft.pulmo_2",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/pulmo2_icon.png",
        "backgroundColor": "#002f56"
      },
      "permissions": ["VIBRATE", "NOTIFICATIONS", "USER_FACING_NOTIFICATIONS"]
    }
  }
}
